<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<definition name="Template"
		template="/WEB-INF/jsp/tiles/DefaultTemplate/template.jsp">
		<put-attribute name="title" value="Home" />

		<put-attribute name="header"
			value="/WEB-INF/jsp/tiles/DefaultTemplate/Header.jsp" />
		<put-attribute name="titlepage"
			value="/WEB-INF/jsp/tiles/DefaultTemplate/titlePage.jsp" />
		<put-attribute name="sidebar"
			value="/WEB-INF/jsp/tiles/DefaultTemplate/SideBar.jsp" />
		<put-attribute name="page" value="" />
		<put-attribute name="footer"
			value="/WEB-INF/jsp/tiles/DefaultTemplate/Footer.jsp" />
	</definition>
	<definition name="hello" extends="Template">
	<put-attribute name="page" value="/WEB-INF/jsp/page/hello.jsp" />
	</definition>
</tiles-definitions>